	"version": "0.0.5",
	"license": "MIT",
	"type": "module",
	"bin": {
		"lava": "lava-console/index.js"
	},
	"author": "Joshua Davis <chess.ofx@gmail.com> (https://josh-davis.dev)",
	"keywords": [
		"lava",
		"Joshua Davis",
		"Markdown Generation",
		"md",
		"obsidian"
	],
	"files": [
